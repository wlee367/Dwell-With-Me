{
  "expo": {
    "name": "dwell-with-me",
    "slug": "dwell-with-me",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "React Native chat application that only works between 20% and 85% battery life. Stay anonymous or announce your name to the world. ",
    "githubUrl": "https://github.com/wlee367/Dwell-With-Me"
  }
}